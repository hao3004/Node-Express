extends ../layouts/main

block create
    img(alt="meow cute", src="data:image/png;base64,")
    h1.float-left Create User

    if errors
        each error in errors
            .alert.alert-danger= error
    form.form-inline(action="/users/create", method="POST")
        .input-group.mb-2.mr-sm-2
            .input-group-prepend
                .input-group-text
                    i.far.fa-id-card
            input.form-control(type='text', name="name", value=values?values.name:'', placeholder='Name')
        .input-group.mb-2.mr-sm-2
            .input-group-prepend
                .input-group-text
                    i.fas.fa-blender-phone
            input.form-control(type='tel', name="phone", placeholder='Phone')
        .input-group.mb-2.mr-sm-2
            button.btn.btn-info Create